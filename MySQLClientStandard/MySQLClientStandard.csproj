<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <AssemblyName>DewMySQLClient</AssemblyName>
    <RootNamespace>DewCore.DewDatabase.MySQL</RootNamespace>
    <Authors>Andrea Vincenzo Abbondanza</Authors>
    <Company>Dew Studio</Company>
    <Product>Dew MySQL Client</Product>
    <Description>A client for mysql for .net standard, using mysql connector</Description>
    <PackageProjectUrl>https://github.com/andreabbondanza/MySQLClient</PackageProjectUrl>
    <RepositoryUrl>https://github.com/andreabbondanza/MySQLClient</RepositoryUrl>
    <PackageLicenseUrl>https://github.com/andreabbondanza/MySQLClient/blob/master/README.md</PackageLicenseUrl>
    <PackageTags>MySQL .net standard</PackageTags>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageIconUrl>http://andrewdev.eu/wp-content/uploads/2016/08/OnlyLogo.png</PackageIconUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DewInterfaces" Version="1.0.5" />
    <PackageReference Include="DewLogger" Version="1.0.8" />
    <PackageReference Include="MySqlConnector" Version="0.14.0" />
  </ItemGroup>

</Project>